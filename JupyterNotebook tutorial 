        "# Multiply and add numbers\n",
        "a = 5\n",
        "b = 3\n",
        "c = 2\n",
        "\n",
        "# Multiplication\n",
        "product = a * b\n",
        "\n",
        "# Addition\n",
        "sum_result = product + c\n",
        "\n",
        "# Display results\n",
        "print(\"Multiplication Result:\", product)\n",
        "print(\"Final Result after Addition:\", sum_result)"
      ],
      "metadata": {
